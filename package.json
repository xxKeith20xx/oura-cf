		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.53.1",
		"vitest": "~3.2.0",
		"wrangler": "^4.60.0"
	}
