{
	"name": "oura-vault",
	"version": "1.0.1",
	"private": true,
	"scripts": {
		"deploy": "wrangler deploy",
		"deploy:cf": "wrangler d1 migrations apply oura-db --remote && wrangler deploy",
		"dev": "wrangler dev",
		"start": "wrangler dev",
		"test": "vitest",
		"test:run": "vitest --run",
		"lint": "eslint .",
