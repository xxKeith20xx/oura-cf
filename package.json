{
	"name": "oura-vault",
	"version": "1.0.5",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/xxKeith20xx/oura-cf.git"
	},
	"bugs": {
		"url": "https://github.com/xxKeith20xx/oura-cf/issues"
	},
	"homepage": "https://github.com/xxKeith20xx/oura-cf#readme",
	"keywords": [
		"oura-ring",
		"cloudflare-workers",
		"d1-database",
		"grafana",
		"health-data",
		"fitness-tracking"
	],
	"engines": {
		"node": ">=22.0.0"
	},
	"scripts": {
		"deploy": "wrangler deploy",
		"deploy:cf": "wrangler d1 migrations apply oura-db --remote && wrangler deploy",
		"dev": "wrangler dev",
		"start": "wrangler dev",
		"test": "vitest",
		"test:run": "vitest --run",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"cf-typegen": "wrangler types",
		"check": "npm run lint && npm run format:check && npm run test:run"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.12.6",
		"@cloudflare/workers-types": "^4.20260124.0",
