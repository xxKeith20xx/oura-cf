	"name": "oura-vault",
	"main": "src/index.ts",
	"compatibility_date": "2025-12-25",
	"triggers": {
    "crons": ["0 9 * * *"] // Runs daily at 9:00 AM
  },
	"observability": {
		"enabled": true
	},
	"d1_databases": [
		{
			"binding": "oura_db",
			"database_name": "oura-db",
			"database_id": "5672d3e9-19d4-4faf-b997-7e6afc1f34be",
			"remote": true
		}
	]
}
